{
        "python.analysis.diagnosticMode": "workspace",
        "python.analysis.typeCheckingMode": "basic",
        "python.formatting.provider": "black",
        "python.formatting.blackPath": "black",
        "python.linting.mypyEnabled": true,
        "python.linting.pylintEnabled": true,
        "emmet.includeLanguages": {
                "javascript": "javascriptreact",
                "svelte": "html"
        },
        "typescript.autoClosingTags": true,
        "javascript.autoClosingTags": true,
        "svelte.enable-ts-plugin": true,
        "eslint.packageManager": "yarn",
        "eslint.nodePath": "/usr/bin/node",
        "eslint.autoFixOnSave": true,
        "css.validate": false,
        "less.validate": false,
        "scss.validate": false,
        "wxss.validate": false,
        "Lua.diagnostics.globals": [
                "vim"
        ],
        "tabnine.shortcut": "TabNine",
        "tabnine.priority": 1,
        "suggest.completionItemKindLabels": {
                "method": "  ",
                "function": "  ",
                "variable": "[]",
                "field": "  ",
                "typeParameter": "<>",
                "constant": "  ",
                "class": "  ",
                "interface": " 蘒",
                "struct": "  ",
                "event": "  ",
                "operator": "  ",
                "module": "  ",
                "property": "  ",
                "enum": " 練",
                "reference": "  ",
                "keyword": "  ",
                "file": "  ",
                "folder": " ﱮ ",
                "color": "  ",
                "unit": " 塞 ",
                "snippet": " ",
                "text": "  ",
                "constructor": "  ",
                "value": "  ",
                "enumMember": "  ",
                "default": "  "
        },
        "suggest.highPrioritySourceLimit": 10,
        "suggest.languageSourcePriority": 95,
        "suggest.lowPrioritySourceLimit": 90,
        "coc.source.file.priority": 60,
        "coc.source.buffer.priority": 50,
        "suggest.triggerCompletionWait": 30,
        "suggest.autoTrigger": "always",
        "suggest.localityBonus": true,
        "suggest.fixInsertedWord": true,
        "suggest.enablePreselect": true,
        "suggest.removeDuplicateItems": true,
        "suggest.preferCompleteThanJumpPlaceholder": true,
        "suggest.defaultSortMethod": "alphabetical",
        "suggest.triggerAfterInsertEnter": false,
        "suggest.detailField": "preview",
        "suggest.noselect": false,
        "diagnostic.errorSign": " ",
        "diagnostic.warningSign": " ",
        "diagnostic.infoSign": "",
        "diagnostic.hintSign": " ",
        "coc.preferences.formatOnType": true,
        "codeLens.enable": false,
        "pyright.inlayHints.enable": false,
        "pyright.inlayHints.functionReturnTypes": false,
        "pyright.inlayHints.variableTypes": false,
        "diagnostic.virtualText": false,
        "languageserver": {
                "csharp-ls": {
                        "command": "csharp-ls",
                        "filetypes": [
                                "cs"
                        ],
                        "rootPatterns": [
                                "*.csproj",
                                ".vim/",
                                ".git/",
                                ".hg/"
                        ]
                }
        }
}
