{
  // Python
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.formatting.provider": "black",
  // "python.formatting.blackPath": "black",
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": true,

  // Rust
  "rust-analyzer.inlayHints.typeHints": false,

  // Java
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-14",
      "path": "/usr/lib/jvm/default",
      "default": true
    }
  ],

  "java.format.enabled": true,
  "java.format.onType.enabled": true,
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.format.comments.enabled": true,
  "java.signatureHelp.enabled": true,
  "java.saveActions.organizeImports": true,
  "java.selectionRange.enabled": true,

  // Emmet
  "emmet.includeLanguages": { "javascript": "javascriptreact" },

  // Javascript and TypeScript
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,

  // Eslint
  "eslint.packageManager": "yarn",
  "eslint.nodePath": "/usr/bin/node",
  "eslint.autoFixOnSave": true,

  // Css
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,

  // Lua
  "Lua.diagnostics.globals": ["vim"],

  // Deno
  // "deno.enable": true,
  // "deno.lint": true,
  // "deno.unstable": true,
  // "prettier.disableLanguages": ["javascript", "typescript", "json"],
  // "tsserver.enable": false,

  // Tabnine
  "tabnine.shortcut": "TabNine",
  "tabnine.priority": 1,

  // Diagnostic Sever
  "diagnostic-languageserver.formatFiletypes": {
    "lua": "lua-format",
    "sh": "shfmt",
    "zsh": "shfmt"
  },

  "diagnostic-languageserver.filetypes": {
    "vim": "vint"
  },

  // Snippets
  "snippets.priority": 1,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets/ultisnips",
  "snippets.shortcut": "S",
  "snippets.autoTrigger": true,
  "snippets.triggerCharacters": [],
  "snippets.loadFromExtensions": true,
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/snippets/textmate"],
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.usePythonx": true,
  "snippets.ultisnips.directories": ["UltiSnips"],
  "snippets.snipmate.enable": true,
  "snippets.snipmate.author": "Eliaz Bobadilla",

  // Yank
  "yank.limit": 50,
  "yank.list.maxsize": 100,

  // Completion Icons
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": "  ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": " ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  ",
    "default": "  "
  },

  "suggest.highPrioritySourceLimit": 10,
  "suggest.languageSourcePriority": 95,
  "suggest.lowPrioritySourceLimit": 90,

  "coc.source.file.priority": 60,
  "coc.source.buffer.priority": 50,
  "yank.priority": 10,
  "suggest.triggerCompletionWait": 30,

  "suggest.autoTrigger": "always",
  "suggest.localityBonus": true,
  "suggest.fixInsertedWord": true,
  "suggest.enablePreselect": true,
  "suggest.removeDuplicateItems": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.numberSelect": false,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.detailField": "menu",
  "suggest.keepCompleteopt": false,
  "suggest.noselect": false,

  // Linting Icons
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",

  // CoC
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnType": true,

  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",

  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    }
  },
  "cSpell.userWords": [
    "Ashcraft",
    "Batata",
    "Bobadilla",
    "DDLC",
    "Doki",
    "Dotfiles",
    "Eliaz",
    "Gmail",
    "MERN",
    "Platzi",
    "Quizizz",
    "Ren'Py",
    "Rustacean",
    "Ryuk",
    "Salvato",
    "Salvato's",
    "Ulti",
    "Zlyrics",
    "aeiou",
    "alphanum",
    "asyncio",
    "autogenerated",
    "autoload",
    "azapi",
    "azlyrics",
    "bindgen",
    "camelcase",
    "chika",
    "classmap",
    "consts",
    "copero",
    "deno",
    "diah",
    "dotenv",
    "dpage",
    "euphi",
    "fatih",
    "gclal",
    "gclone",
    "inmemory",
    "kratos",
    "lelouch",
    "middlewares",
    "minmax",
    "mongodb",
    "monika",
    "moongose",
    "mypy",
    "nats",
    "natsuki",
    "pgadmin",
    "phpunit",
    "postgresql",
    "preconnect",
    "psql",
    "pydantic",
    "pymongo",
    "randomstring",
    "sayori",
    "snippetmode",
    "struct",
    "styl",
    "ultirequiem",
    "uvicorn",
    "waitlist"
  ]
}
